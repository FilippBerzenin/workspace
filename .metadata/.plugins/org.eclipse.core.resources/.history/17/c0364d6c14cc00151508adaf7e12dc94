package Lesson_3;

public class frame_3_3 {

	public static void main(String[] args) {
		
		int [] date = {10, -3, 25, 4};

		swap(date, 3, 3);
		swap(date, -1, 3);
		swap(date, 2, 5);
		}

		public static void swap (int[] date, int i, int k) {
			if (k<date.length && i<date.length && date != null) {
			System.out.println("before: "+date[i]+" i");
			System.out.println("before: "+date[k]+" k");	
			int memory = date[i];
			date[i] = date[k];
			date[k] = memory;
			System.out.println("after: "+date[i]+" i");
			System.out.println("after: "+date[k]+" k");
			} if (i>date.length-1 || k>date.length-1 || date == null)
			{System.out.println(-1);
			}
			}
	}

