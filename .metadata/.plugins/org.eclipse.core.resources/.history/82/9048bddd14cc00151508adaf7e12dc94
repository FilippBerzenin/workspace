package Lesson_3;

public class frame_3_3 {

	public static void main(String[] args) {
		
		int [] date = {4, 5, 6, 8};

		swap(date, 3, 3);
		swap(date, -1, 3);
		swap(date, 2, 5);
		}

		public static void swap (int[] date, int i, int k) {
			if (i>date.length-1 || k>date.length-1 || date == null)
				{System.out.println(-1);
			}
			else {
				int memory = date[i];
				date[i] = date[k];
				date[k] = memory;
			}
		}
	}

