package Lesson_3;

public class test {

	public static void main(String[] args) {

        int[] arr = {1, 2, 3, 4, 5};
        int[] arr1 = {4, 2};
        int[] arr2 = {1};
   //     int[] arr4 = null;
        
       

        System.out.println(oddElementSum(arr));
        System.out.println(oddElementSum(arr1));
        System.out.println(oddElementSum(arr2));
   //     System.out.println(oddElementSum(arr4));
      
    }

    static double oddElementSum(int data[]) {
    	if (data == null) {
    		return -1;
    	} double rezult;
    	for (double index : data) {
    		rezult += index;
    	} return = rezult/data.length;

    }

}
