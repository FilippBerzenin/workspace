package Lesson_3;

import java.util.Arrays;

public class frame_3_3 {

	public static void main(String[] args) {
		
		int [] date = {4, 5, 6, 8};

		System.out.println(Arrays.toString(swap(date, 3, 3)));
		swap(date, -1, 3);
		swap(date, 2, 5);
		}

		public static int[] swap (int[] date, int i, int k) {
			if ((i>date.length-1 && i>=0) || (k>date.length-1 && k>=0) || date == null)
				{System.out.println(-1);
			}
			else {
				int mem = 0;
				mem = date[i];
				date[i] = date[k];
				date[k] = mem;
			}
			return date;
		}
	}

